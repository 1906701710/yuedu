.container{
  background-color: #21272e;
  padding-bottom: 200rpx;
}
.comment-nav-wrapper{
  text-align: center;
  width: 680rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding-right: 20px;
}
.back-wrapper{
  float: left;
}
.mymsg-wrap{
  overflow: hidden;
  width: 680rpx;
  margin: 0 auto;
  padding-bottom: 40rpx;
  margin-top: 40rpx;
}
.header-photo{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  float: left;
}
.adress-wrap{
  float: left;
  margin-top: 20rpx;
  margin-left: 20rpx;
  
}
.name-wrap{
  font-size: 40rpx;
}
.adress{
  color: #b5b7c5;
  font-size: 27rpx;
}
.share-wrap{
  float: right;
  border: 1rpx solid #545d66;
  color: #9497ab;
  margin-top: 30rpx;
  padding-bottom: 10rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  border-radius: 10rpx;
}
.details-wrap{
  width: 680rpx;
  display: flex;
  margin: 0 auto;
  justify-content: space-between;
  background-color: #2b323b;
  padding: 20rpx 0px;
  border-radius: 20rpx;
  text-align: center;
}
.details{
  border-right: 1px solid #363c45;
  padding: 0rpx 70rpx;
}
.details-sole{
  border-right: none;
}
.my-details{
  font-size: 27rpx;
  color: #75808d;
}
.isattention-wrapper{
  padding: 10rpx 50rpx;
  background-color: #478ce0;
  display: inline-block;
  border-radius: 10rpx;
  margin-top: 138rpx;
  margin-left: 20rpx;
}
.status-wrap{
  width: 680rpx;
  margin: 0 auto;
  overflow: hidden;
  padding: 40rpx 0rpx;
  
}
.status-left{
  float: left;
}
.status-right{
  float: right;
  font-size: 25rpx;
  color: #a2a8ba;
  line-height: 20px;
}
.see-all-wrap{
  color: #478ce0;
}
.book-content-wrap{
  width: 680rpx;
  margin: 0 auto;
  overflow: hidden;
  margin-top: 40rpx;
  padding-bottom: 40rpx;
  border-bottom: 1px solid #2f343c;
}
.works-photo-wrapper{
  float: left;
  width: 250rpx;
  height: 350rpx;
}
.works-particulars-wrapper{
  width: 430rpx;
  float: left;
  box-sizing: border-box;
  padding-top: 30rpx;
  padding-left: 30rpx;
}
.works-particulars-name{
  font-size: 50rpx;
}
.works-particulars-introduce{
  color: #868dab;
  margin-top: 10rpx;
  font-size: 30rpx;
  height: 80rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;

}
.works-particulars-score{
  margin-top: 30rpx;
  display: inline-block;
}
.works-particulars-score-star-wrap{
  float: left;
}
.works-particulars-score-num{
  float: left;
  color: #ffce44;
  margin-left: 20rpx;
  font-size: 40rpx;
}
.reading-photo-wrapper{
  font-size: 27rpx;
  color: #478ce0;
  margin-top: 20rpx;
}